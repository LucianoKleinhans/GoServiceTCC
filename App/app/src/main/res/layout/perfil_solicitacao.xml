<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#071219"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">

    <View
        android:id="@+id/myRectangle0View3"
        android:layout_width="wrap_content"
        android:layout_height="160dp"
        android:background="@drawable/retangle_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/ttvUsernamePerfilSolicita"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:layout_marginStart="150dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="5dp"
        android:text="@string/user_name"
        android:textColor="#d5d5d5"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/ttvEmailPerfilSolicita"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_marginStart="150dp"
        android:layout_marginEnd="5dp"
        android:text="@string/seu_email"
        android:textColor="#A8A8A8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ttvCidadePerfilSolicita" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/ttvCidadePerfilSolicita"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_marginStart="150dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="5dp"
        android:text="@string/cidade_uf"
        android:textAlignment="viewStart"
        android:textColor="#A8A8A8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myRectangleView25" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/ttvSitePerfilSolicita"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_marginStart="150dp"
        android:layout_marginEnd="5dp"
        android:text="@string/seu_site"
        android:textColor="#A8A8A8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ttvTelefonePerfilSolicita" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/ttvTelefonePerfilSolicita"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_marginStart="150dp"
        android:layout_marginEnd="5dp"
        android:text="seu_telefone"
        android:textColor="#A8A8A8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ttvEmailPerfilSolicita" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:contentDescription="TODO"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_logogoservice" />

    <View
        android:id="@+id/myRectangleView25"
        android:layout_width="0dp"
        android:layout_height="15dp"
        android:layout_marginStart="150dp"
        android:layout_marginEnd="5dp"
        android:background="@drawable/retangle_border_view_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ttvUsernamePerfilSolicita" />

    <ImageView
        android:id="@+id/star_1"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/myRectangleView25"
        app:layout_constraintEnd_toStartOf="@+id/star_2"
        app:srcCompat="@drawable/ic_star" />

    <ImageView
        android:id="@+id/star_2"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/myRectangleView25"
        app:layout_constraintEnd_toStartOf="@+id/star_4"
        app:srcCompat="@drawable/ic_star" />

    <ImageView
        android:id="@+id/star_3"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginStart="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/myRectangleView25"
        app:layout_constraintStart_toEndOf="@+id/star_4"
        app:srcCompat="@drawable/ic_star" />

    <ImageView
        android:id="@+id/star_4"
        android:layout_width="15dp"
        android:layout_height="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/myRectangleView25"
        app:layout_constraintEnd_toEndOf="@+id/myRectangleView25"
        app:layout_constraintHorizontal_bias="0.199"
        app:layout_constraintStart_toStartOf="@+id/myRectangleView25"
        app:srcCompat="@drawable/ic_star" />

    <ImageView
        android:id="@+id/star_5"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginStart="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/myRectangleView25"
        app:layout_constraintStart_toEndOf="@+id/star_3"
        app:srcCompat="@drawable/ic_star" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="15dp"
        android:background="#4bb2f8"
        android:backgroundTint="#4bb2f8"
        android:clickable="true"
        android:onClick="btn_perfilprestador_to_main"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_home"
        tools:ignore="SpeakableTextPresentCheck" />

    <View
        android:id="@+id/myRectangleView21"
        android:layout_width="0dp"
        android:layout_height="180dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="15dp"
        android:background="@drawable/retangle_border_view_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myRectangle0View3" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/ttvValorServicoPerfilSolicita"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="5dp"
        android:background="@drawable/retangle_border_view_5"
        android:text="Valor atual : R$80"
        android:textAlignment="center"
        android:textColor="#d5d5d5"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/btnSegueNegociacaoPerfilSolicita"
        app:layout_constraintEnd_toEndOf="@+id/myRectangleView21"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/myRectangleView21" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/ttvDescServicoPerfilSolicita"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="5dp"
        android:inputType="textMultiLine"
        android:text="DescServico"
        android:textColor="#A8A8A8"
        app:layout_constraintBottom_toTopOf="@+id/ttvValorServicoPerfilSolicita"
        app:layout_constraintEnd_toEndOf="@+id/myRectangleView21"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/myRectangleView21"
        app:layout_constraintTop_toBottomOf="@+id/ttvServicoPerfilSolicita" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/ttvServicoPerfilSolicita"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="5dp"
        android:text="Nome servico"
        android:textColor="#A8A8A8"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/myRectangleView21"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/myRectangleView21"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView21" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btnSegueNegociacaoPerfilSolicita"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="5dp"
        android:backgroundTint="#30739f"
        android:onClick="btn_perfil_to_cadservico"
        android:text="Seguir para negocioção"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="@+id/myRectangleView21"
        app:layout_constraintEnd_toEndOf="@+id/myRectangleView21"
        app:layout_constraintStart_toStartOf="@+id/myRectangleView21" />

</androidx.constraintlayout.widget.ConstraintLayout>